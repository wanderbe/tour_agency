        <body-content>empty</body-content>
        <info></info>
        <attribute>
            <name>email</name>
            <required>true</required>
            <description>Checking if email correct written</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
     <tag>
        <name>validateLogin</name>
        <tag-class>ua.nure.sidorovich.jstl.ValidateUserLoginTag</tag-class>
        <body-content>empty</body-content>
        <info></info>
        <attribute>
