            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
      <tag>
        <name>amountUsersHaveHot</name>
        <tag-class>ua.nure.sidorovich.jstl.AmountUsersHaveHotTourTag</tag-class>
        <body-content>empty</body-content>
        <info></info>
        <attribute>
            <name>orderStatusId</name>
            <required>true</required>
            <description>Information how many users have HOT tours by status</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
</taglib>