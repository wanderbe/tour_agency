        <body-content>empty</body-content>
        <info></info>
        <attribute>
            <name>idUser</name>
            <required>true</required>
            <description>Checking if user blocked using id</description>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
     <tag>
        <name>validateEmail</name>
